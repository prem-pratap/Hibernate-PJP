<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wipro.bean.Employee" table="employee">
		<id name="empId" column="empid" type="int">
		<generator class="increment"></generator></id>
		
		<property name="empName" column="empname" type="string"></property>
		<property name="designation" column="designation" type="string"></property>
		<property name="salary" column="salary" type="int"></property>
		
	
	</class>
</hibernate-mapping>